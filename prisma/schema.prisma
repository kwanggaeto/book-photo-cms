// This file is automatically generated by Prisma

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Photo {
  id        Int      @id @default(autoincrement())
  uid       String   @unique @default(uuid())
  filename  String
  createdAt DateTime @default(now())
  expiresAt DateTime
  size      Int
  mimeType  String
}
